{
  "name": "react-with-throttle",
  "version": "0.0.5",
  "description": "React component wrapper for throttling value",
  "author": "Albert Lucianto <albertlucianto@gmail.com>",
  "main": "dist/react-with-throttle.umd.js",
  "module": "dist/react-with-throttle.esm.js",
  "unpkg": "dist/react-with-throttle.min.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlbertLucianto/react-with-throttle.git"
  },
  "bugs": {
    "url": "https://github.com/AlbertLucianto/react-with-throttle/issues"
  },
  "homepage": "https://github.com/AlbertLucianto/react-with-throttle",
  "keywords": [
    "react",
    "throttle"
  ],
  "scripts": {
    "unit": "cross-env NODE_ENV=test jest --config __test__/jest.conf.js --coverage",
    "test": "npm run unit",
    "dev": "cross-env NODE_ENV=development TARGET=desktop webpack-dev-server --inline --progress --hot --mode development --config build/webpack.config.js",
    "build:web": "rimraf dist/web && cross-env NODE_ENV=production webpack --inline --progress --mode production --config build/webpack.config.js",
    "build:browser": "rollup --config build/rollup.browser.js",
    "build:es": "rollup --config build/rollup.es.js",
    "build:umd": "rollup --config build/rollup.umd.js",
    "build": "npm run build:browser && npm run build:es && npm run build:umd && npm run build:web",
    "coverage": "codecov -f __test__/coverage/coverage-final.json"
  },
  "dependencies": {
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "autoprefixer": "^9.4.10",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-react-css-modules": "^5.2.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "classnames": "^2.2.6",
    "codecov": "^3.2.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "is-number": "^7.0.0",
    "jest": "^24.1.0",
    "node-sass": "^4.11.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-scss": "^2.0.0",
    "postcss-url": "^8.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.8.0",
    "react-testing-library": "^6.0.0",
    "react-with-throttle": "^0.0.5",
    "rimraf": "^2.6.3",
    "rollup": "^1.4.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify": "^6.0.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglify-es": "^3.3.9",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.2.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  }
}
